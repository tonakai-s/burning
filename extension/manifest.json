{
    "manifest_version": 3,
    "name": "Button Listener Example",
    "version": "1.0",
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["tabs", "scripting"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content-script.js"]
        }
    ]
}
